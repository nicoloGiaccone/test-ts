{
    "extends": "./tsconfig.paths.json",
    "compilerOptions": {
        "target": "es5",
        "strictNullChecks": true,
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "alwaysStrict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve"
    },
    "include": [
        "next-env.d.ts",
        "**/*.ts",
        "**/*.tsx",
        ".eslintrc.js"
, "next.config.js"    ],
    "exclude": [
        "node_modules"
    ]
}